<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="marc.hi.ehrs.svc.core" type="MARC.HI.EHRS.SVC.Core.Configuration.HostConfigurationSectionHandler, MARC.HI.EHRS.SVC.Core"/>
    <section name="santedb.core" type="SanteDB.Server.Core.Configuration.ConfigurationSectionHandler, SanteDB.Server.Core"/>
  </configSections>

  <santedb.core>
    <threading poolSize="16"/>
  </santedb.core>
  <marc.hi.ehrs.svc.core>
    <serviceProviders>
      <add type="SanteDB.Server.Core.Services.Impl.ThreadPoolService, SanteDB.Server.Core"/>
    </serviceProviders>
  </marc.hi.ehrs.svc.core>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2"/>
  </startup>
</configuration>
